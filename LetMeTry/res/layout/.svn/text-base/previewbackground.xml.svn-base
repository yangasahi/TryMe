<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
android:id="@+id/widget40"
android:layout_width="fill_parent"
android:layout_height="fill_parent"
xmlns:android="http://schemas.android.com/apk/res/android"
>
<LinearLayout
android:id="@+id/widget27"
android:layout_width="fill_parent"
android:layout_height="40px"
android:background="#ff333333"
xmlns:android="http://schemas.android.com/apk/res/android"
android:layout_alignParentTop="true"
android:layout_alignParentLeft="true"
>
<LinearLayout
android:id="@+id/widget31"
android:layout_width="200px"
android:layout_height="fill_parent"
android:background="#ff333333"
xmlns:android="http://schemas.android.com/apk/res/android"
>
<Button
android:id="@+id/returnButton"
android:layout_width="55px"
android:layout_height="fill_parent"
android:background="#ff333333"
android:text="返回"
android:textSize="16sp"
android:textColor="#ffffffff"
>
</Button>
<TextView
android:id="@+id/widget40"
android:layout_width="145px"
android:layout_height="fill_parent"
android:background="#ff333333"
android:text="(5.请预览选中的背景，也可拖动调整大小)"
android:textSize="13sp"
>
</TextView>
</LinearLayout>
<LinearLayout
android:id="@+id/widget32"
android:layout_width="120px"
android:layout_height="fill_parent"
android:background="#ff333333"
xmlns:android="http://schemas.android.com/apk/res/android"
>
<Button
android:id="@+id/homeButton"
android:layout_width="65px"
android:layout_height="fill_parent"
android:background="#ff333333"
android:text="再来一次"
android:textSize="16sp"
android:textColor="#ffffffff"
>
</Button>
<Button
android:id="@+id/helpButton"
android:layout_width="55px"
android:layout_height="fill_parent"
android:background="#ff333333"
android:text="Help"
android:textSize="16sp"
android:textColor="#ffffffff"
>
</Button>
</LinearLayout>
</LinearLayout>
<LinearLayout
android:id="@+id/mainShowActivity"
android:layout_width="fill_parent"
android:layout_height="345px"
android:background="#ffffffff"
xmlns:android="http://schemas.android.com/apk/res/android"
android:layout_below="@+id/widget27"
android:layout_alignParentLeft="true"
>
 <ImageView
        android:id="@+id/imageView1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/ic_launcher" />
</LinearLayout>
<LinearLayout
android:id="@+id/widget29"
android:layout_width="fill_parent"
android:layout_height="45px"
android:background="#ff333333"
xmlns:android="http://schemas.android.com/apk/res/android"
android:gravity="bottom"
android:layout_below="@+id/mainShowActivity"
android:layout_alignParentLeft="true"
>
<Button
android:id="@+id/saveButton"
android:layout_width="100px"
android:layout_height="fill_parent"
android:background="#ff333333"
android:text="保存文件"
android:textSize="18sp"
android:textColor="#ffffffff"
>
</Button>
<Button
android:id="@+id/shareButton"
android:layout_width="73px"
android:layout_height="fill_parent"
android:background="#ff333333"
android:text="共享"
android:textSize="18sp"
android:textColor="#ffffffff"
>
</Button>
<Button
android:id="@+id/adornButton"
android:layout_width="73px"
android:layout_height="fill_parent"
android:background="#ff333333"
android:text="装饰"
android:textSize="18sp"
android:textColor="#ffffffff"
>
</Button>
<Button
android:id="@+id/tryonButton"
android:layout_width="73px"
android:layout_height="fill_parent"
android:background="#ff333333"
android:text="试穿"
android:textSize="18sp"
android:textColor="#ffffffff"
>
</Button>
</LinearLayout>
</RelativeLayout>

